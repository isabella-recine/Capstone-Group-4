     "title": ""
    }
   },
   "outputs": [],
   "source": [
    "# Fill null values based on assumptions \n",
    "yellow_taxi_df = yellow_taxi_df.fillna({'passenger_count':1, 'congestion_surcharge':0, 'Airport_fee':0})\n",
    "green_taxi_df = green_taxi_df.fillna({'passenger_count':1, 'congestion_surcharge':0})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
     "cellMetadata": {
      "byteLimit": 2048000,
