   "source": [
    "# Create datetime column called \"date\" based on the pickup date\n",
    "taxi_df = taxi_df.withColumn(\"date\", taxi_df[\"pickup_datetime\"].cast(\"date\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
