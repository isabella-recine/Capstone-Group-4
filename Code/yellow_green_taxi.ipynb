   "source": [
    "# Read Yellow and Green Taxi DataFrames and drop duplicate rows\n",
    "yellow_taxi_df = spark.read.option(\"recursiveFileLookup\", \"true\").parquet(\"s3://capstone-techcatalyst-raw/yellow_taxi/*\",multiLine=True, encoding=\"utf8\").drop_duplicates()\n",
    "green_taxi_df = spark.read.option(\"recursiveFileLookup\", \"true\").parquet(\"s3://capstone-techcatalyst-raw/green_taxi/*\",multiLine=True, encoding=\"utf8\").drop_duplicates()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
     "cellMetadata": {
      "byteLimit": 2048000,
